{
    "hosting":  {
                    "ignore":  [
                                   "firebase.json",
                                   "**/.*",
                                   "**/node_modules/**"
                               ],
                    "public":  "v22_root/build/web",
                    "headers":  [
                                    {
                                        "headers":  [
                                                        {
                                                            "key":  "Access-Control-Allow-Origin",
                                                            "value":  "*"
                                                        },
                                                        {
                                                            "key":  "Access-Control-Expose-Headers",
                                                            "value":  "Content-Length"
                                                        },
                                                        {
                                                            "key":  "Cache-Control",
                                                            "value":  "no-cache"
                                                        }
                                                    ],
                                        "source":  "**/*.data"
                                    }
                                ]
                }
}
